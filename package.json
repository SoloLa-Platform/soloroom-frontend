{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --colors --coverage",
    "test:watchAll": "vue-cli-service test:unit --watchAll --notify --colors --coverage",
    "test:watch": "vue-cli-service test:unit --colors --watch --coverage",
    "start": "node server.js & vue-cli-service serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "axios": "^0.19.0",
    "core-js": "3.5.0",
    "cors": "^2.8.5",
    "opensheetmusicdisplay": "^0.7.1",
    "soundfont-player": "^0.12.0",
    "store": "^2.0.12",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vue-slider-component": "^3.1.2",
    "vue-youtube": "^1.4.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/store": "^2.0.2",
    "@vue/cli-plugin-babel": "4.1.1",
    "@vue/cli-plugin-typescript": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "body-parser": "^1.20.1",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "husky": ">=4",
    "jest": "26.0.0",
    "jest-transform-stub": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": ">=10",
    "node-sass": "^4.13.1",
    "prettier": "^2.1.2",
    "readline": "^1.3.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^26.4.1",
    "typescript": "4.0.3",
    "vue-template-compiler": "^2.6.10",
    "xml": "^1.0.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(js|ts)?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "/node_modules/(?!(opensheetmusicdisplay)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)",
      "**/(*.)+(spec|test).[jt]s?(x)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,ts,js,json,html,css,md}": "prettier --write"
  }
}
